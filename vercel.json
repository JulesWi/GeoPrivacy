{
  "version": 2,
  "name": "geoprivacy",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "frontend/pages/index.tsx"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_ENV": "production"
  },
  "github": {
    "silent": true
  }
}
