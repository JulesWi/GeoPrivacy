{
  "version": 2,
  "name": "geoprivacy",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "src/backend/server.ts",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/backend/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/"
    }
  ],
  "github": {
    "silent": true
  }
}
